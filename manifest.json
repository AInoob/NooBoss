{
  "manifest_version": 2,
  "name": "NooBoss",
  "description": "Manage your extensions, apps, and themes, recommends extensions and scripts personally, and warns you bad reputation extensions",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": "images/icon_128.png",
    "default_title": "NooBoss~~~",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/icon_128.png"
  },
  "background": {
    "scripts":["js/background.js"]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "permissions":[
    "management",
    "notifications"
  ]
}
