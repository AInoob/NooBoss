{
  "manifest_version": 2,
  "name": "NooBoss",
  "description": "Manage your extensions, apps, and themes, recommends extensions and scripts personally, and warns you bad reputation extensions",
  "version": "0.0.8",
  "browser_action": {
    "default_icon": "images/icon_128.png",
    "default_title": "NooBoss~~~",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "images/icon_128.png"
  },
  "background": {
    "scripts":["thirdParty/jquery.min.js","js/util.js","js/background.js"]
  },
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
  "optional_permissions": [ "tabs"],
  "permissions":[
    "management",
    "notifications",
    "storage",
    "unlimitedStorage",
    "https://clients2.google.com/*",
    "https://chrome.google.com/*",
    "https://ainoob.com/*"
  ]
}
