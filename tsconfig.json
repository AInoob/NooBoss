{
  "compilerOptions": {
    "experimentalDecorators": true,
    "outDir": "./dist/",
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "lib": [ "es2015", "dom" ],
    "exclude": [
      "node_modules",
      "thirdParty"
    ],
    "moduleResolution": "node"
  }
}